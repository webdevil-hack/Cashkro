{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://cashkaro-backend.onrender.com",
    "NEXT_PUBLIC_APP_URL": "https://cashkaro-clone.vercel.app"
  }
}