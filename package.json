{
  "name": "cashkaro-clone",
  "version": "1.0.0",
  "description": "A comprehensive clone of CashKaro.com with full functionality",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "cd server && npm run dev",
    "client": "cd client && npm run dev",
    "build": "cd client && npm run build",
    "start": "cd server && npm start",
    "install-all": "npm install && cd server && npm install && cd ../client && npm install",
    "deploy:vercel": "cd client && vercel --prod",
    "deploy:railway": "cd server && railway up",
    "make-live": "./make_live.sh"
  },
  "keywords": ["cashback", "shopping", "deals", "coupons", "ecommerce"],
  "author": "CashKaro Clone Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}